[{
    "name": "aurora-db",
    "cpu": 256,
    "memory": 128,
    "essential": true,
    "portMappings": [{"containerPort": 80, "hostPort": 443}],

    "secrets": [{
        "name": "DATABASE_PASSWORD",
        "valueFrom": "arn:aws:ssm:ap-southeast-2:*:parameter/wordpress/WORDPRESS_DB_USER"
    }],
    "mountPoints": [{
        "containerPath": "../wordpress"
    }]
}]
